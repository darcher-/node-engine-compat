{
  "author": "Dylan Archer <dylarcher@gmail.com> (@darcher-)",
  "bin": {
    "check-node-compat": "./src/index.js"
  },
  "bugs": "https://github.com/darcher-/nodeVersionRange/issues",
  "description": "A CLI library to check the defined node version engines of the project and its dependencies",
  "devDependencies": {
    "axe-core": "4.10.3",
    "c8": "^10.1.3",
    "eslint": "8.57.0",
    "htmlhint": "1.2.0",
    "markdownlint-cli": "0.45.0",
    "prettier": "3.5.3",
    "standard-version": "^9.5.0",
    "stylelint": "16.20.0",
    "yargs": "18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false,
    "vueIndentScriptAndStyle": false,
    "htmlWhitespaceSensitivity": "css",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "insertPragma": false,
    "tabWidth": 2
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.14.0"
  },
  "files": [
    "src",
    "README.md",
    "CHANGELOG.md"
  ],
  "gitHead": "b1c0f3d8c4e2a5f6b7e8c9d0e1f2a3b4c5d6e7f8",
  "homepage": "https://darcher-.github.io/nodeVersionRange/",
  "keywords": [
    "compatibility",
    "engines",
    "highest",
    "levels",
    "limits",
    "lowest",
    "max",
    "maximum",
    "min",
    "minimum",
    "node",
    "nodejs",
    "range",
    "semver",
    "support",
    "version"
  ],
  "license": "MIT",
  "main": "./src/index.js",
  "name": "node-compat-range-analyzer",
  "packageManager": "npm@10.9.2",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:darcher-/nodeVersionRange.git",
  "scripts": {
    "release": "standard-version",
    "start": "node ./src/index.js",
    "test": "node -tr=spec test/*.test.js",
    "test:coverage": "c8 -c=.github/linters/.c8rc.json -ai=src/**/*.js -- npm test"
  },
  "source": "src/demo/index.html",
  "type": "module",
  "version": "1.1.0",
  "dependencies": {
    "semver": "^7.7.2"
  }
}
